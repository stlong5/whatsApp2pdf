{
  "name": "whatsapp2pdf",
  "version": "1.0.1",
  "description": "Convert WhatsApp chat export to PDF File",
  "type": "commonjs",
  "main": "index.js",
  "exports": {
    ".": "./index.js"
  },
  "bin": {
    "whatsapp-pdf": "./cli.js",
    "whatsapp2pdf": "./cli.js"
  },
  "scripts": {
    "start": "node cli.js",
    "test": "node test.js",
